*{
    margin: 0;  
    padding: 0;
}
html{
    font-size: 100px;
}
img{
    width: 100%;
}
a{
    display: block;
    text-decoration: none;
}
.title{
    margin: 16px 0 14px 0;
}
.title .title-text{
    font-size: .32rem;
    color: #333;
    line-height: .32rem;
    text-align: center;
    margin-bottom: .24rem;
    font-weight: 700;
}
.title .title-line{
    width: .48rem;
    height: .04rem;
    background: #ccaa7a;
    margin: 0 auto;
}
/* 每一个头部结束 */

/* 每一个轮播图开始 */
.channel-nine .content{
    width: 7.5rem;
    height: 5rem;
}
.channel-nine .content .swiper-container .swiper-wrapper .banner{
    width: 7.5rem;
    display: block;
}

/* 分页器样式开始 */
.channel-nine .content .swiper-pagination .swiper-pagination-bullet{
    background: #fff;
    opacity: 1;
}
.channel-nine .content .swiper-pagination .swiper-pagination-bullet-active{
    background-color: #ccaa7a;
    width: .4rem;
    border-radius: .1rem;
}
/* 分页器样式结束 */

/* 每一个轮播图结束 */

/* 品牌下面的店铺开始 */
.channel-eight{
    width: 7.5rem;
}
.channel-eight .content{
    width: 100%;
    display: flex;
}
.channel-eight .content .img{
    width: 33.33%;
    display: block;
}
/* 品牌下面的店铺结束 */

/* 本月爆款开始 */
.channel-three{
    width: 7.5rem;
}
.channel-three .content a{
    width: 100%;
    height: 3.6rem;
}
.bom{
    margin-bottom: .54rem;
}
/* 本月爆款开始 */

/* 闪购下面的打折店铺开始 */
.channel-ten{
    width: 7.5rem;
}
.channel-ten .content{
    margin: 20px 0 0 8px;
}
.channel-ten .content .shop .swiper-wrapper .shop-img{
    width: 3.1rem !important;
    margin-right: .16rem;
}
/* 闪购下面的打折店铺结束 */
