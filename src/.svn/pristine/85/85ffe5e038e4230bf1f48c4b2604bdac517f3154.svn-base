*{
    margin: 0;
    padding: 0;
}
html{
    font-size: 100px;
}
/* 头部开始 */
.header{
    width: 7.5rem;
    height: .46rem;
    border: .01rem solid rgb(218, 215, 215);
    display: flex;
    justify-content: space-around;
    align-items : center;
    position: fixed;
    background-color: white;
}
.header .header_b{
    position: absolute;
    top: .08rem;
    left: .1rem;
    background: rgba(202, 202, 201, 0.3);
    width: 3.57rem;
    height: .28rem;
    color: #999;
    border-radius: .26rem;
}
.header .header_b span{
    position: absolute;
    left: .5rem;
    font-size: .05rem;
    line-height: .3rem;
}
.header .header_b i{
    position: absolute;
    top: .05rem;
    left: .1rem;
    font-size: .2rem;
}
.header_c {
    display: block;
    background-image: url(../images/brandstore/d.png);
    background-position: 0 -3.73rem;
    background-size: 8.57rem;
    width: .88rem;
    height: .88rem;
}
/* 头部结束 */
/* 侧边栏导航 */
.xuan-left{
    overflow: hidden;
    width:.85rem;
    height: 6.2rem;
    border: .01rem solid rgb(218, 215, 215);
    border-top: hidden;
    position: fixed;
    top:.46rem;
}

.xuan-left ul{
    position: fixed;
    top:.46rem;
}
.xuan-left li{
    width: .85rem;
    height: .5rem;
    font-size: .1rem;
    /* border-bottom: .01rem solid gray; */
    text-align: center;
    line-height: .5rem;
    color: rgb(49, 48, 48);
}
.xuan-left .hover span{
    display: inline-block;
    line-height: .25rem;
    background-color: rgba(0, 0, 0, 0.8);
    border-radius: .5rem;
    width: .65rem;
    color: white;
    height: .25rem;
}
/* 侧边栏导航结束 */
/* 内容 */
.xuan-right{
    width: 2.9rem;
    height: 6.2rem;
    margin-left: .85rem;
    font-size: .1rem;
}
.xuan-right .xuan-content .xuan-con1 img{
    width: 2.65rem;
    height: 1.05rem;
    margin-top: .55rem;
    margin-left: .13rem;
}
.xuan-content .xuan-nav{
    display: inline-block;
    margin-top: .1rem;
    margin-left: 1rem;
    color: rgb(175, 173, 173);
}
.xuan-right .xuan-content .xuan-con2 p{
    color: rgb(88, 87, 87);
}
.xuan-right .xuan-content .xuan-con2 .xuan-cont{
    margin-top: .2rem;
}
.xuan-right .xuan-content .xuan-con2 .xuan-cont li{
    width: .95rem;
    height: 1rem;
    float: left;
    vertical-align: middle;
}
.xuan-right .xuan-content .xuan-con2 .xuan-cont li img{
    width: .6rem;
    height: .6rem;
    display: block;
    margin: 0 auto;
}
.xuan-right .xuan-content .xuan-con2 .xuan-cont1 li img{
    width: .6rem;
    height: .25rem;
    display: block;
    margin: 0 auto;
}
.xuan-right .xuan-content .xuan-con2 .xuan-cont li p{
    margin:0 auto;
    text-align: center;
}
.xuan-right .xuan-content .xuan-con2 .xuan-cont1 li p{
    margin:.25rem auto;
    text-align: center;
}
/* 内容结束 */