*{
    margin: 0;
    padding: 0;
}
html{
    font-size: 100px;
}
main img{
    width: 100%;
}
a{
    text-decoration: none;
}
/* 顶部开始 */
main .head .title{
    width: 100%;
    background-color: #262626;
    border-bottom: .05rem solid #d1af67;
    padding: .3rem 0;
    display: flex;
}
main .head .title a{
    display: flex;
    margin-top: .1rem;
}
main .head .title a .icon{
    background: url(../image/left-jt.png) no-repeat 0 0;
    background-size: cover;
    width: .39rem;
    height: .39rem;
    margin: 0 .1rem 0 .2rem;
}
main .head .title a .text{
    font-size: .28rem;
    color: #fff;
    line-height: .34rem;
}
main .head .title .logo{
    font-size:.4rem;
    font-weight: 700;
    color: #e0b577;
    margin-left: 1.8rem;
}
/* 顶部结束 */

/* 导航开始 */
main .head .nav{
    padding: .26rem 0;
    border-bottom: 1px solid #f1f1f1;
}
main .head .nav .navi .swiper-wrapper .on{
    color: #d1af67;
}
.head .nav .navi .swiper-wrapper .swiper-slide{
    font-size: .26rem;
    width: 1.44rem;
    text-align: center;
    color: #333;
    font-weight: 700;
}
/* 导航结束 */

/* 内容开始 */
main .fashion{
    padding: .3rem;
    border-bottom: .02rem solid #f1f1f1;
}
.fashion a .title{
    font-size: .32rem;
    color: #333;
    line-height: .48rem;
}
.fashion a .img{
    margin-top: .2rem;
}
.fashion a .img img{
    border-radius: .06rem;
}
.fashion .time{
    display: flex;
    justify-content: space-between;
    font-size: .24rem;
    color: #999;
}
/* 内容结束 */
.list-footer{
    font-size: .24rem;
    color: #d1af67;
    padding-top: .28rem;
    text-align: center;
    margin-bottom: .6rem;
}
body .h{
    display: none;
}
/* 尾部导航开始 */
footer{
    width: 7.5rem;
    height: 1.2rem;
}
.foot-content{
    position: fixed;
    bottom: 0;
    left: 0;
    background: #262626;
    display: flex;
    width: 7.5rem;
    height: 1.2rem;
}
.foot-content a{
    width: 25%;
    display: flex;
    flex-direction: column;
    align-items: center;
    margin-top: .1rem;
}
.foot-content a .text{
    font-size: .2rem;
    color: #999;
    text-align: center;
}
footer .foot-content a .on{
    color: #d1af67;
}
.foot-content a .icon-img{
    width: .6rem;
    height: .6rem;
    margin: .08rem 0 .04rem 0;
}
.foot-content a .icon-img1{
    background: url(../image/home-page-1.png) no-repeat 0 0;
    background-size: cover;
}
.foot-content a .icon-img2{
    background: url(../image/fx.png) no-repeat 0 0;
    background-size: cover;
}
.foot-content a .icon-img3{
    background: url(../image/xb.png) no-repeat 0 0;
    background-size: cover;
}
.foot-content a .icon-img4{
    background: url(../image/my.png) no-repeat 0 0;
    background-size: cover;
}
/* 尾部导航结束 */


