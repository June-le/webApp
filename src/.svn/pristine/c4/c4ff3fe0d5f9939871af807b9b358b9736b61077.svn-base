.header{
    width: 7.5rem;
    height: .88rem;
    display: -webkit-flex;
    display: flex;
    align-items: center;
}
.header .add{
    height: .28rem;
    width: 2.28rem;
    padding-left: .32rem;
}
.header .add a{
    display: flex;
    align-items: center;
}
.header .add .icon-d-position {
    display: block;
    background-image: url(../images/mingjiang/d.png);
    background-position: -3.32rem -8.63rem;
    background-size: 9.26rem;
    width: .28rem;
    height: .28rem;
}
.header .add span{
    font-size: .28rem;
    color: #666;
    padding-left: .2rem;
    line-height: .28rem;
    /* padding-right: .15rem; */
    height: .28rem;
    width: 1rem;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    
}
.header .add .icon-d-home_arrow {
    display: block;
    background-image: url(../images/mingjiang/d.png);
    background-position: -2.48rem -3.42rem;
    background-size: 9.26rem;
    width: .2rem;
    height: .2rem;
}
.header .logo{
    display: block;
    background-image: url(../images/mingjiang/d.png);
    background-position: -1.78rem -1.42rem;
    background-size: 9.26rem;
    width: 1.6rem;
    height: .34rem;
    margin-left: .8rem;
}
.swiper-container {
    width: 7.5rem;
    height: 3.24rem;
    position: relative;
   
}
.swiper-container .swiper-wrapper .swiper-slide {
    height: 3rem;
    overflow: hidden;
    box-shadow: 0 .1rem .08rem 0 rgba(31,31,31,.15);
    padding: 0 .32rem;
    box-sizing: border-box;
}
.swiper-container .ban img,.ban1 img{
    width: 6.86rem;
    height: 3rem;
}
.swiper-container .swiper-pagination.swiper-pagination-bullets{
    height: .06rem;
    width: 100%;
    position: absolute;
    left: 0;
    bottom: 0;
    display: flex;
    flex-direction: row;
    justify-content: center;
}
.swiper-pagination-bullet {
    background: #d8d8d8;
    border-radius: .03rem;
    width: .32rem;
    height: .06rem;
    margin-right:.08rem;
    
}
.swiper-container .swiper-pagination-bullet-active{
    background-color: #ccaa7a;
}
