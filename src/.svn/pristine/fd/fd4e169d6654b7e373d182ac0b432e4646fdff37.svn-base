body {
    background: #fffffe!important;
}
header{
    position: relative;
    width: 7.5rem;
    height: .88rem;
    line-height: .88rem;
    background: #fff;
    border-bottom: .02rem solid #ddd;
    box-shadow: 0 2px 3px 0 rgba(0,0,0,.1);
    border-bottom: 0;
}
header .hd-back{
    position: absolute;
    top: 0;
    left: 0;
    width: .88rem;
    height: .88rem;
}
header .hd-back a{
    display: block;
    background-image: url(../images/mingjiang/a.png);
    background-position: -1.96rem -3.06rem;
    background-size: 4.22rem;
    width: .88rem;
    height: .88rem;
}
header .hd-title{
    width: 100%;
    text-align: center;
    height: .88rem;
    line-height: .88rem;
    font-size: .32rem;
    color: #444;
}

main{
    display: flex;
}
main .city-box{
    width: 6.88rem;
    margin-right: 31px;
}
.city-box h5.postion {
    background: #fff;
    box-shadow: inset 0 -.02rem 0 0 #f7f7f7;
    font-size: .36rem;
    color: #666;
    /* line-height: .8rem; */
    margin: .4rem 0 0 .24rem;
    display: flex;
    align-items: center;
}
.city-box .postion .icon-d-position {
    display: inline-block;
    margin-right:.12rem;
    background-image: url(../images/mingjiang/d.png);
    background-position: -3.32rem -8.63rem;
    background-size: 9.26rem;
    width: .28rem;
    height: .28rem;
}
.city-box .W_hotCity {
    margin-top: 8px;
    margin-bottom: .4rem;
}
.W_hotCity h6 {
    margin-left: .24rem;
    line-height: .72rem;
    font-size: .24rem;
    color: #666;
    font-weight: 400;
}
.city-box .W_hotCity ul {
    padding-left: .24rem;
    overflow: hidden;
    box-sizing: border-box;
    margin-top: .16rem;
    display: flex;
    flex-wrap: wrap;
}
.city-box .W_hotCity ul li {
    width: 28.7%;
    border: .02rem solid #ccc;
    border-radius: .06rem;
    line-height: .68rem;
    font-size: .28rem;
    text-align: center;
    margin-right: 4.45%;
    margin-bottom: .16rem;
}
.city-box .W_hotCity ul li a{
    color: #333;    
}
.city-box .city-list{
    padding-left: .32rem;
    height: 13rem;
    overflow: hidden;
}
.city-box .city-list  ul li h4{
    line-height: .72rem;
    font-size: .32rem;
    color: #333;
}
.city-box .city-list  ul li p{
    line-height: .88rem;
    font-size: .24rem;
   
}
.city-box .city-list a{
    color: #666;
}
main .city-index{
    position: fixed;
    top: 50%;
    width: .58rem;
    right: 0;
    transform: translateY(-50%);
}
main .city-index ul li{
    font-size: .24rem;
    color: #cbaf94;
    text-align: center;
    line-height: .36rem;
    display: block;
}