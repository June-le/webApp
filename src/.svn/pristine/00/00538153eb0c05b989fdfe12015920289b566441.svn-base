    display: block
}

.W_footer img {
    width: 100%
}

.W_footer {
    width: 100%;
    max-width: 640px;
    background: #fff;
    height: 49px;
    border-top: 1px solid #f1f1f1;
    position: fixed;
    bottom: 0;
    z-index: 997
}

.W_footer a {
    width: 20%;
    float: left;
    padding-top: 5px;
    color: #666;
    position: relative
}

.W_footer a>.W_footer_img {
    margin: 0 auto;
    margin-bottom: 3px
}

.W_footer a>.W_footer_img img {
    width: 40%;
    margin-left: 30%
}

.W_footer a>.W_footer_title {
    width: 80%;
    margin: 0 auto;
    font-size: 10px;
    line-height: 14px;
    text-align: center;
    color: #333
}

.W_footer a>.W_footer_tag {
    position: absolute;
    background: #d90000;
    border: 1px solid #fff;
    border-radius: 100px;
    padding: 0 4px;
    top: 2px;
    right: 13px;
    color: #fff;
    font-size: 12px;
    text-align: center;
    line-height: 14px
}

